{
  "type": "module",
  "name": "daytona-mvc",
  "version": "0.1.2",
  "description": "Daytona: The JavaScript MVC framework with batteries included. Build using convention over configuration.",
  "main": "index.js",
  "scripts": {
    "start": "NODE_ENV=production node index.js",
    "staging": "NODE_ENV=staging node index.js",
    "dev": "NODE_ENV=development nodemon dev.js",
    "test": "ormond",
    "encrypt": "topsecret encrypt config.toml config.toml.secret --key-file config.key",
    "import-all": "nodemon --watch . -- --preserve-symlinks --preserve-symlinks-main import-all.js"
  },
  "directories": {
    "lib": "lib",
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/miketerry-org/daytonajs.git"
  },
  "keywords": [
    "daytona",
    "mvc",
    "server",
    "mongodb",
    "mysql",
    "postgres",
    "sqlite"
  ],
  "author": "Mike Terry",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/miketerry-org/daytonajs/issues"
  },
  "homepage": "https://github.com/miketerry-org/daytonajs#readme",
  "dependencies": {
    "@faker-js/faker": "^10.1.0",
    "@iarna/toml": "^2.2.5",
    "better-sqlite3": "^12.4.6",
    "compression": "^1.8.1",
    "connect-redis": "^9.0.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "express-fileupload": "^1.5.2",
    "express-rate-limit": "^8.2.1",
    "express-session": "^1.18.2",
    "helmet": "^8.1.0",
    "hpp": "^0.2.3",
    "install": "^0.13.0",
    "libsql": "^0.5.22",
    "method-override": "^3.0.0",
    "mongodb": "^6.21.0",
    "morgan": "^1.10.1",
    "mysql2": "^3.15.3",
    "pg": "^8.16.3",
    "pluralize": "^8.0.0",
    "redis": "^5.9.0",
    "sql-where-parser": "^2.2.1"
  },
  "devDependencies": {
    "cross-env": "^7.0.3",
    "nodemon": "^3.1.10"
  }
}
